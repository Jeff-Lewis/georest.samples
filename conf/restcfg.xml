<RestConfig>
	<Data>
		<Resource uripart="">
		    
			<Source type="MapGuide">
				<FeatureSource>hjhghjg</FeatureSource>
				<FeatureClass>hghg</FeatureClass>
				<ServerIp>127.0.0.1</ServerIp>
				<ServerPort>2812</ServerPort>
			</Source>
			
			
			<Representation renderer="JSON" pattern=".json"/>
			
			
			<Representation renderer="XML" pattern=".xml"/>
			 
			
			<Representation renderer="Sitemap" pattern="sitemap.xml" />
			
			
			
			<Representation renderer="Template" pattern=".html" mimetype="text/html" >
				<Templates section="" prefix="">
					<Many file="_html_many.tpl" />
					<Single file="_html_single.tpl" />										
					<None file="_html_none.tpl" />
					<Error file="_html_error.tpl" />
				</Templates>
				<Method name="GET" />
			</Representation>
			
			
			<Representation renderer="Template" pattern=".kml" mimetype="application/vnd.google-earth.kml+xml" >
				<Templates section="" prefix="">
					<Many file="_kml_many.tpl" />
					<Single file="_kml_single.tpl" />										
					<None file="_kml_none.tpl" />
					<Error file="_kml_error.tpl" />				
				</Templates>
				<Method name="GET" maxcount="500" />
			</Representation>
			
			
			<Representation renderer="Template" pattern=".kmz" mimetype="application/vnd.google-earth.kmz" >
				<Templates section="" prefix="">
					<Many file="_kml_many.tpl" />
					<Single file="_kml_single.tpl" />										
					<None file="_kml_none.tpl" />
					<Error file="_kml_error.tpl" />			
				</Templates>
				<Method name="GET" maxcount="500" />
			</Representation>			
			
		</Resource>
	
	</Data>
</RestConfig>
